<div class="container">
  <div class="row">
    <div class="col">
      <div class="col-md-4 col-md-offset-4">
        <h1>Appointments</h1>
        <%= link_to "Edit", edit_appointment_path(@appointment) %>
        <%= link_to appointment_path(@appointment), method: :delete, data: { confirm: "Are you sure?" } do %>
        <i class="fa fa-trash destroy_dose">Delete</i>
        <% end %>
      </div>
    </div>
  </div>
</div>
